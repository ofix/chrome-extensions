{
    "manifest_version": 3,
    "name": "Redfish",
    "version": "1.0.0",
    "description": "抓取Redfish API接口返回的数据",
    "homepage_url": "https://github.com/ofix",
    "author": "songhuabiao@greatwall.com.cn",
    "action": {
        "default_icon": {
            "24": "assets/16_16.png",
            "48": "assets/48_48.png",
            "64": "assets/64_64.png",
            "128": "assets/128_128.png"
        },
        "default_title": "BMC Redfish APIs",
        "default_popup": "popup/index.html",
        "default_css": "popup/popup.css"
    },
    "background": {
        "service_worker": "background/service_worker.js"
    },
    "content_scripts": [
        {
            "js": [
                "content/content.js"
            ],
            "matches": [
                "https://*/*"
            ],
            "all_frames": true,
            "run_at": "document_end",
            "match_about_blank": true
        }
    ],
    "permissions": [
        "scripting",
        "bookmarks",
        "browsingData",
        "cookies",
        "nativeMessaging",
        "contextMenus",
        "tabs",
        "notifications",
        "webRequest",
        "storage",
        "declarativeNetRequest",
        "declarativeNetRequestFeedback",
        "declarativeNetRequestWithHostAccess",
        "alarms",
        "desktopCapture",
        "management"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "content_pages/*",
                "assets/*",
                "_locales/*"
            ],
            "matches": [
                "https://*/*"
            ],
            "use_dynamic_url": true
        }
    ],
    "commands": {
        "download_redfish_pages": {
            "suggested_key": {
                "default": "Ctrl+Q",
                "windows": "Ctrl+Q",
                "mac": "Command+Q",
                "chromeos": "Ctrl+Q",
                "linux": "Ctrl+Q"
            },
            "description": "保存 BMC Redfish API 接口数据抓取"
        },
        "reload_extension": {
            "suggested_key": {
                "default": "Alt+R",
                "windows": "Alt+R",
                "mac": "Alt+R"
            },
            "description": "reload browser extension"
        }
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "omnibox": {
        "keyword": "api"
    },
    "default_locale": "zh_CN"
}